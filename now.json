{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/eufavicon.png$",
      "dest": "/eufavicon.png"
    },
    {
      "src": "^/icons/minus.svg$",
      "dest": "/icons/minus.svg"
    },
    {
      "src": "^/icons/plus.svg$",
      "dest": "/icons/plus.svg"
    },
    {
      "src": "^/icons/stats.svg$",
      "dest": "/icons/stats.svg"
    },
    {
      "src": "^/icons/warning-check.svg$",
      "dest": "/icons/warning-check.svg"
    },
    {
      "src": "^/euStars.png$",
      "dest": "/euStars.png"
    },
    {
      "src": "^/robots.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
